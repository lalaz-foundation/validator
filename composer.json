{
  "name": "lalaz/validator",
  "description": "Fluent validation engine with 40+ built-in rules and custom validators.",
  "type": "library",
  "require": {
    "php": "^8.3",
    "lalaz/framework": "^1.0.0-rc.1",
    "psr/container": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Lalaz\\Validator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lalaz\\Validator\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
